{
  "name": "astraea-fabrication",
  "version": "1.0.0",
  "type": "module",
  "description": "High-performance landing page for Astraea Fabrication countertop installation",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy && node scripts/generate-sitemap.js",
    "preview": "eleventy --serve --pathprefix=''",
    "analyze": "ANALYZE=true eleventy",
    "clean": "rm -rf _site .11ty-vite .cache"
  },
  "keywords": ["eleventy", "vite", "static-site", "landing-page"],
  "author": "Astraea Fabrication",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-vite": "^7.0.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "html-minifier": "^4.0.0",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "rollup-plugin-critical": "^1.0.15",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.0",
    "vite": "^7.1.11",
    "vite-plugin-compression2": "^2.3.0",
    "vite-plugin-image-optimizer": "^2.0.2"
  }
}
